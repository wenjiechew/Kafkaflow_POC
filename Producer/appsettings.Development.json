{
  "FileSinkOptions": {
    "RetainedFileCountLimit": 1,
    "FilePath": "Logs/log-.txt",
    "OutputTemplate": "[{CorrelationId}][{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
  },
  "Kafka": {
    "Brokers": "localhost:9092",
    "Producer": {
      "Topic": "topic-1",
      "Partitions": 1,
      "ReplicationFactor": 1
    }
  }
}
